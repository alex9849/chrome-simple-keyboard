{
  "$schema": "https://json.schemastore.org/chrome-manifest.json",
  "manifest_version": 3,
  "version": "0.5.6",
  "name": "simple-virtual-keyboard",
  "chromium:key": "",
  "description": "A native virtual keyboard based on the simple-keyboard package.",
  "icons": {
    "48": "images/icon_128.png"
  },
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "options_ui": {
    "page": "content/options/options.html",
    "open_in_tab": true
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_idle",
      "js": [
        "content/keyboard/contentScript.js"
      ],
      "css": [
        "content/keyboard/contentScript.css"
      ]
    }
  ],
  "author": "Alex9849"
}